        "rtconner/laravel-tagging": "~3.2",
        "jenssegers/date": "^3.5",
        "laravelium/feed": "~6.0",
        "graham-campbell/markdown": "^11.1.0",
        "webuni/commonmark-attributes-extension": "1.0.0",
        "laravelcollective/html": "^6.0",
        "vinkla/hashids": "^7.0.0",
        "aws/aws-sdk-php": "~3.0",
        "spatie/laravel-newsletter": "^4.6",
        "michelf/php-markdown": "^1.8",
        "spatie/eloquent-sortable": "^3.7",
        "spatie/laravel-translatable": "^4.2",
        "spatie/regex": "^1.4",
        "rap2hpoutre/laravel-log-viewer": "^1.3",
        "spatie/laravel-feed": "~2.4",
        "mpociot/versionable": "dev-master",
        "nonoesp/thinker": "6.x-dev",
        "mtownsend/read-time": "^1.1",
        "symfony/intl": "^5.1@dev",
        "nasyrov/laravel-imgix": "dev-master",
        "spatie/dropbox-api": "^1.12",
        "spatie/laravel-sitemap": "^5.7.0",
        "rezozero/commonmark-ext-footnotes": "dev-master",
        "spatie/flysystem-dropbox": "^1.2.2",
        "league/flysystem-aws-s3-v3": "^1.0@dev",
        "spatie/laravel-backup": "^6.8.0"
